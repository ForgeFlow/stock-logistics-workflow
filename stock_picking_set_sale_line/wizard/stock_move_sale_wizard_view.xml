<odoo>
    <!-- wizard view -->
    <record id="stock_move_sale_line_wizard_view" model="ir.ui.view">
        <field name="name">stock.move.sale.wizard</field>
        <field name="model">stock.move.sale.wizard</field>
        <field name="arch" type="xml">
            <form string="Change related Sale Line">
                <group name="sale_line" string="Sale Lines">
                    <field name="item_ids" >
                        <tree string="Sale Line" nocreate="1" editable="top" widget="many2many">
                            <field name="select_me"/>
                            <field name="sequence2"/>
                            <field name="product_id"/>
                            <field name="commitment_date"/>
                            <field name="product_uom_qty"/>
                        </tree>
                    </field>
                </group>
                <footer>
                    <button
                        string="Apply"
                        name="change_sale_line"
                        type="object"
                        class="btn-primary"
                    />
                    <button string="Cancel" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
